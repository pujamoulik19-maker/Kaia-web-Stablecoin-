<script>
const contractAddress=" 0xYOUR_DEPLOYED_CONTRACT";
const abi = [
"function balance Of(address)view returns (unit256)"
];
let provider, signer, contract;
document.getElementById("connect").onclick =async () =>{
provider = new ethers.providers.Web3 Provider(window.etherum);
signer = provider.getSigner();
contract = new ethers.Contract (contract Addres, abi, signer);
document.getElementById ("output").innerText= "Connected"!;
} else {
alert (" Raby not found");
document.getElementById ("balance").onclick = async () => {
const address = awaitsigner.getAddress();
const bal = await connect.balanceOf (address);
document.getElementId("output").innerText = "Balance: " +bal.toString();
};
</script>
